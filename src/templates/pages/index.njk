{% extends "layouts/base.njk" %}
{% block content %}
    <style>
        .worksheet {
            width: 1200px;
            padding: 20px;
        }
        table {
            table-fixed: layout;
            width: 100%;
        }
        tr {
            display: grid;
            grid-template-columns: 13% 13% 13% 1fr 10% 15%;
            border-bottom: 1px solid #d6d6d6;
        }
        thead tr {
            border-bottom: 1px solid #4c4c4c;
        }
        thead th {
            padding: 14px 16px;
            background: #f4f5f6;
        }
        tbody tr {
            border-bottom: 1px solid #d6d6d6;
        }
        tr.depth-1 {
            background: #f4f5f6;
        }
        td {
            padding: 12px 14px;
        }
        td:nth-child(5) {
            text-align: center;
        }
        td a {
            word-wrap: break-word;
        }
        .--progress span {
            color: #C78500;
        }
        .--progress span::before {
            content: '진행';
        }
        .--complete span {
            position: relative;
        }
        .--complete span::before {
            content: '완료';
        }
        .--complete span::after {
            content: '';
            display: inline-block;
            position: absolute;
            top: 7px;
            left: 0;
            width: 100%;
            height: 10px;
            background: #84c860;
            filter: opacity(0.3);
            z-index: -1;
        }
        @media(max-width:565px) {
            #container {
                margin: 20px auto;
            }
            .worksheet {
                width: 100%;
            }
            h1 {
                margin-bottom: 20px;
                font-size: 2rem;
            }
            thead {
                display: none;
            }
            tbody {
                border-top: 1px solid #4c4c4c;
            }
            tbody tr {
                display: flex;
                flex-wrap: wrap;
                padding: 6px 0 0;
            }
            tr.depth-1 {
                padding: 3px 0;
                background: #f4f5f6;
                border-bottom: 1px solid #4c4c4c;
            }
            tr.depth-1 td {
                display: block;
                flex: 0 0 100%;
            }
            td {
                padding: 3px 5px;
            }
            td:nth-child(1),
            td:nth-child(2),
            td:nth-child(3) {
                width: 16%;
            }
            td:nth-child(4) {
                display: block;
                width: 40%;
            }
            td:nth-child(5) {
                width: 12%;
                text-align: left;
            }
            td:nth-child(6) {
                width: 100%;
                padding: 3px 8px;
            }
            td:nth-child(6) p {
                position: relative;
                display: inline;
            }
            td:nth-child(6) p::after {
                content: '';
                display: inline-block;
                position: absolute;
                bottom: 0;
                left: 0;
                width: 100%;
                height: 10px;
                background: #5bc5d2;
                filter: opacity(0.3);
                z-index: -1;
            }
        }
    </style>
